{
  "name": "@brentlintner/vile-slim-lint",
  "version": "0.2.0",
  "description": "A vile plugin for slim-lint",
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib",
    "src"
  ],
  "scripts": {
    "compile": "babel -d lib -m common src",
    "compile-dev": "bin/compile-dev",
    "dev": "bin/dev",
    "test": "bin/test",
    "test-cov": "bin/test-cov",
    "vile": "vile -cspd --nodecorations"
  },
  "keywords": [
    "vile",
    "slim",
    "lint",
    "ruby"
  ],
  "dependencies": {
    "@brentlintner/vile": "~0.5.7",
    "bluebird": "~3.1.5",
    "lodash": "~3.10.1"
  },
  "devDependencies": {
    "@brentlintner/vile-coffeelint": "0.1.2",
    "@brentlintner/vile-comment": "0.1.1",
    "@brentlintner/vile-coverage": "^0.2.0",
    "@brentlintner/vile-depcheck": "0.1.0",
    "@brentlintner/vile-eclint": "0.1.0",
    "@brentlintner/vile-eslint": "0.1.0",
    "@brentlintner/vile-fs-lint": "^0.3.0",
    "@brentlintner/vile-git": "^0.2.0",
    "@brentlintner/vile-jshint": "0.1.0",
    "@brentlintner/vile-language": "0.1.0",
    "@brentlintner/vile-license": "0.3.1",
    "@brentlintner/vile-ncu": "0.1.3",
    "@brentlintner/vile-newline": "0.1.0",
    "@brentlintner/vile-nsp": "^0.2.1",
    "@brentlintner/vile-semver": "0.1.1",
    "@brentlintner/vile-whitespace": "0.1.0",
    "babel": "~5.8.34",
    "chai": "~3.5.0",
    "chai-as-promised": "^5.2.0",
    "coffee-script": "~1.10.0",
    "istanbul": "^0.4.2",
    "mimus": "~0.1.2",
    "mocha": "~2.4.5",
    "release-it": "~2.3.1",
    "sinon": "~1.17.3",
    "sinon-chai": "~2.8.0"
  }
